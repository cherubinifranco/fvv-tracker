{
  "version": "0.0.1",
  "name": "tracker",
  "productName": "Tracker",
  "author": "Franco Cherubini",
  "description": "App to track work done",
  "private": true,
  "repository": "",
  "publish": {
    "provider": "github",
    "private": false,
    "owner": "cherubinifranco",
    "repo": "fvv-tracker",
    "releaseType": "release"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.tracker.fvv",
    "artifactName": "Tracker-Setup-${version}.${ext}",
    "files": [
      "**/*",
      "build"
    ],
    "win": {
      "icon": "/icon.ico",
      "target": "nsis"
    }
  },
  "main": "public/main.js",
  "homepage": "./",
  "dependencies": {
    "desktop-screenshot": "^0.1.1",
    "electron-updater": "^6.3.9",
    "path": "^0.12.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "^6.14.1",
    "react-router-dom": "^6.14.1",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "electron": "^26.2.1",
    "electron-builder": "^25.1.8",
    "tailwindcss": "^3.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
